@@ -63,7 +63,6 @@ static int mei_spd_probe(struct mei_cl_device *cldev,
 
 	spd_dbg(spd, "protocol version %d\n", ver);
 	mei_spd_gpp_prepare(spd);
-	mei_spd_rpmb_prepare(spd);
 	mutex_lock(&spd->lock);
