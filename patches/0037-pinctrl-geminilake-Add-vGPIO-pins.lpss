+	PINCTRL_PIN(94, "vCNV_GNSS_UART_TXD"),
+	PINCTRL_PIN(95, "vCNV_GNSS_UART_RXD"),
+	PINCTRL_PIN(96, "vCNV_GNSS_UART_CTS_B"),
