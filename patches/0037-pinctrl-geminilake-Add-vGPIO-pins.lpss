+	PINCTRL_PIN(108, "vLPSS_UART2_CTS_B"),
+	PINCTRL_PIN(109, "vLPSS_UART2_RTS_B"),
+	PINCTRL_PIN(110, "vCNV_GNSS_HOST_WAKEB"),
