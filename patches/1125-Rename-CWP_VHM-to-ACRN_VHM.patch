-obj-$(CONFIG_CWP_VHM) 		+= vhm/
+obj-$(CONFIG_ACRN_VHM) 		+= vhm/
 obj-$(CONFIG_ACRN)		+= acrn/
diff --git a/drivers/char/Makefile b/drivers/char/Makefile
index 3c4b742..522e71c 100644
