-	if (cwp_hvlog->sbuf != NULL) {
-		ret = sbuf_get(cwp_hvlog->sbuf, (uint8_t *)&data);
-		if (ret > 0) {
-			if (copy_to_user(buf, &data, ret))
-				return -EFAULT;
-		}
-
-		return ret;
-	}
-
-	return 0;
-}
