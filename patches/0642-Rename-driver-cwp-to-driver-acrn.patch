-
-
-#define TRACE_SBUF_SIZE		(4 * 1024 * 1024)
-#define TRACE_ELEMENT_SIZE	32 /* byte */
-#define TRACE_ELEMENT_NUM	((TRACE_SBUF_SIZE - SBUF_HEAD_SIZE) /	\
-				TRACE_ELEMENT_SIZE)
-
-#define foreach_cpu(cpu, cpu_num)					\
-	for ((cpu) = 0; (cpu) < (cpu_num); (cpu)++)
-
