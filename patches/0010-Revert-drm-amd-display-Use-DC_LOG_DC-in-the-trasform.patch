From 7fd4df6ff86c697e33483d67c964351caeb255b4 Mon Sep 17 00:00:00 2001
From: Ranjan Dutta <ranjan.dutta@intel.com>
Date: Tue, 20 Jun 2023 05:27:41 +0800
Subject: [PATCH 10/30] Revert "drm/amd/display: Use DC_LOG_DC in the trasform
 pixel function"

This reverts commit d5138ad7ca1d85ceac0cb76961f834b54c1a5549.
---
 drivers/gpu/drm/amd/display/dc/dce/dce_transform.c | 5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/drivers/gpu/drm/amd/display/dc/dce/dce_transform.c b/drivers/gpu/drm/amd/display/dc/dce/dce_transform.c
index a54a309879246..e2e79025825f8 100644
--- a/drivers/gpu/drm/amd/display/dc/dce/dce_transform.c
+++ b/drivers/gpu/drm/amd/display/dc/dce/dce_transform.c
@@ -1011,7 +1011,7 @@ static void dce_transform_set_pixel_storage_depth(
 		color_depth = COLOR_DEPTH_101010;
 		pixel_depth = 0;
 		expan_mode  = 1;
-		DC_LOG_DC("The pixel depth %d is not valid, set COLOR_DEPTH_101010 instead.", depth);
+		BREAK_TO_DEBUGGER();
 		break;
 	}
 
@@ -1025,7 +1025,8 @@ static void dce_transform_set_pixel_storage_depth(
 	if (!(xfm_dce->lb_pixel_depth_supported & depth)) {
 		/*we should use unsupported capabilities
 		 *  unless it is required by w/a*/
-		DC_LOG_DC("%s: Capability not supported", __func__);
+		DC_LOG_WARNING("%s: Capability not supported",
+			__func__);
 	}
 }
 
