From 066bd2495ddf7bf9d895604af75089bf151e6ba2 Mon Sep 17 00:00:00 2001
From: "Zhou, Furong" <furong.zhou@intel.com>
Date: Wed, 17 Jul 2024 08:41:31 +0800
Subject: [PATCH 02/76] Enable more kernel options

Enable USB/ICE/IGC/NPU/QAT kernel options

Signed-off-by: Zhou, Furong <furong.zhou@intel.com>
---
 Intel/config | 38 +++++++++++++++++++-------------------
 1 file changed, 19 insertions(+), 19 deletions(-)

diff --git a/Intel/config b/Intel/config
index 6986e297f82c1..a6131d158d992 100644
--- a/Intel/config
+++ b/Intel/config
@@ -2450,9 +2450,9 @@ CONFIG_MD_MULTIPATH=m
 # CONFIG_MD_CLUSTER is not set
 # CONFIG_BCACHE is not set
 CONFIG_BLK_DEV_DM_BUILTIN=y
-CONFIG_BLK_DEV_DM=m
+CONFIG_BLK_DEV_DM=y
 # CONFIG_DM_DEBUG is not set
-CONFIG_DM_BUFIO=m
+CONFIG_DM_BUFIO=y
 # CONFIG_DM_DEBUG_BLOCK_MANAGER_LOCKING is not set
 CONFIG_DM_BIO_PRISON=m
 CONFIG_DM_PERSISTENT_DATA=m
@@ -2478,7 +2478,7 @@ CONFIG_DM_DELAY=m
 # CONFIG_DM_DUST is not set
 CONFIG_DM_UEVENT=y
 CONFIG_DM_FLAKEY=m
-CONFIG_DM_VERITY=m
+CONFIG_DM_VERITY=y
 # CONFIG_DM_VERITY_VERIFY_ROOTHASH_SIG is not set
 CONFIG_DM_VERITY_FEC=y
 # CONFIG_DM_SWITCH is not set
@@ -2674,9 +2674,9 @@ CONFIG_I40E=m
 CONFIG_I40E_DCB=y
 CONFIG_IAVF=m
 CONFIG_I40EVF=m
-# CONFIG_ICE is not set
+CONFIG_ICE=m
 CONFIG_FM10K=m
-# CONFIG_IGC is not set
+CONFIG_IGC=m
 CONFIG_JME=m
 CONFIG_NET_VENDOR_LITEX=y
 CONFIG_NET_VENDOR_MARVELL=y
