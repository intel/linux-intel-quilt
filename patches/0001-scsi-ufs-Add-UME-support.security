+		ret = ufshcd_uma_suspend(hba);
+		if (ret)
+			return ret;
+		uma_suspended = true;
+	}
 
 	for (retries = UIC_HIBERN8_ENTER_RETRIES; retries > 0; retries--) {
 		ret = __ufshcd_uic_hibern8_enter(hba);
@@ -3926,6 +3984,9 @@ static int ufshcd_uic_hibern8_enter(struct ufs_hba *hba)
