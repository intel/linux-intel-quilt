 #define UFS_MASK(mask, offset)		((mask) << (offset))
@@ -244,6 +253,10 @@ enum {
 #define COMMAND_OPCODE_MASK		0xFF
 #define GEN_SELECTOR_INDEX_MASK		0xFFFF
 
+/* UMA - Unified Memory Area */
+#define UFSHCI_UMA_MNOOUR		GENMASK(2, 0)
